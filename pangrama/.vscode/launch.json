{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch TypeScript",
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "args": [
          "${workspaceFolder}/src/index.ts"
        ],
        "sourceMaps": true,
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  